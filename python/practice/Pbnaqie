# Practice_8_9

def Pbnaqie(n):
           'For math'
           n=int(n)
           if n == 1 or n == 2 :
                 return 1
           else :
                 return Pbnaqie(n-1)+Pbnaqie(n-2)


if __name__ == '__main__':
           m = raw_input('Write Your Number:')
           print '%d' % (Pbnaqie(m))
